struct FILE {
    BigEndian();
    uint16 opcode;
    uint32 AckHandle;
    ubyte IsBuffered;
    ubyte ErrorCode;
    uint16 PacketSize;

    uint16 NumItems;
    uint16 NumItems;
    
    ITEM Items[NumItems] <optimize=false>;
} file;